      "permanent": true
    },
    {
      "source": "/core/:section(actions|connectors)/:name",
      "destination": "/core/api/:section/:name",
      "permanent": true
    },
    {
      "source": "/core/config",
      "destination": "/core/api/createConfig",
      "permanent": true
    },

    {
      "source": "/react",
      "destination": "/react/getting-started",
